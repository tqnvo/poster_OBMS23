%---ARBEITEN MIT PFEILEN------------------------------------------------
%  `Overrightarrow', `Overleftarrow' und `Overleftrightarrow' sind Pfei-
%   le, die ueber dem mathematischen Symbol #1 angeordnet werden.
%  `Underrightarrow' , `Underleftarrow'  und `Underleftrightarrow'  sind
%   Pfeile, die unter dem mathematischen Symbol #1 angeordnet werden.
%  `Xrightarrow' , `Xleftarrow'  und `Xleftrightarrow'  sind Abbildungs-
%   pfeile, bei denen unterhalb (#1) und oberhalb (#2) zentriert  mathe-
%   matische Symbole angeordnet werden koennen.
%   Syntax:
%  \Xoverrightarrow{#1}, \Xoverleftarrow{#1}, \Xoverleftrightarrow{#1},
%  \Xunderrightarrow{#1},\Xunderleftarrow{#1},\Xunderleftrightarrow{#1},
%  \Xrightarrow[#1]{#2}, \Xleftarrow[#1]{#2}, \Xleftrightarrow[#1]{#2}.
%------------------------------------------------------------------------
\newcommand{\Xoverrightarrow}{\mathpalette{\over@arrow@\right@arrowfill@}}
\newcommand{\Xoverleftarrow}{\mathpalette{\over@arrow@\left@arrowfill@}}
\newcommand{\Xoverleftrightarrow}{\mathpalette{\over@arrow@\leftright@arrowfill@}}
\newcommand{\Xunderrightarrow}{\mathpalette{\under@arrow@\right@arrowfill@}}
\newcommand{\Xunderleftarrow}{\mathpalette{\under@arrow@\left@arrowfill@}}
\newcommand{\Xunderleftrightarrow}{\mathpalette{\under@arrow@\leftright@arrowfill@}}
\newcommand{\Xrightarrow}[2][]{%
   \mathrel{\mathop{%
   \setbox\z@\vbox{\m@th%
      \hbox{$\scriptstyle\;{#1}\;\;$}%
      \hbox{$\m@th\scriptstyle\;{#2}\;\;$}}%
   \hbox to\ifdim\wd\z@>11\p@\wd\z@\else 11\p@\fi{%
   \right@arrowfill@\displaystyle}}%
   \limits^{#2}_{#1}}%
}
\newcommand{\Xleftarrow}[2][]{%
   \mathrel{\mathop{%
   \setbox\z@\vbox{\m@th%
      \hbox{$\scriptstyle\;\;{#1}\;$}%
      \hbox{$\m@th\scriptstyle\;\;{#2}\;\;$}}%
   \hbox to\ifdim\wd\z@>11\p@\wd\z@\else 11\p@\fi{%
   \left@arrowfill@\displaystyle}}%
   \limits^{#2}_{#1}}%
}
\newcommand{\Xleftrightarrow}[2][]{%
   \mathrel{\mathop{%
   \setbox\z@\vbox{\m@th%
      \hbox{$\scriptstyle\;\;{#1}\;$}%
      \hbox{$\m@th\scriptstyle\;\;{#2}\;\;$}}%
   \hbox to\ifdim\wd\z@>11\p@\wd\z@\else 11\p@\fi{%
   \leftright@arrowfill@\displaystyle}}%
   \limits^{#2}_{#1}}%
}
\def\over@arrow@#1#2#3{%
   \vbox{\ialign{##\crcr#1#2\crcr%
   \noalign{\kern.1ex\nointerlineskip}$\m@th\hfil#2#3\hfil$\crcr}}%
}
\def\under@arrow@#1#2#3{%
   \vtop{\ialign{##\crcr$\m@th\hfil#2#3\hfil$\crcr%
   \noalign{\nointerlineskip\kern.4ex}#1#2\crcr}}%
}
\def\right@arrowfill@#1{%
   \m@th\setbox\z@\hbox{$#1\relbar$}\ht\z@\z@%
   $#1\copy\z@\mkern-6mu\cleaders%
   \hbox{$#1\mkern-2mu\box\z@\mkern-2mu$}\hfill%
   \mkern-6mu\mathord\rightarrow$%
}
\def\left@arrowfill@#1{%
   \m@th\setbox\z@\hbox{$#1\relbar$}\ht\z@\z@%
   $#1\mathord\leftarrow\mkern-6mu\cleaders%
   \hbox{$#1\mkern-2mu\copy\z@\mkern-2mu$}\hfill%
