\def\@tableofcontentsp{\@ifnextchar[\tableofcontentsp@i{\tableofcontentsp@ii}}
%...Definitionen zur Bestimmung der vertikalen Zeilenabstaende..........
\newskip\@toc@lineskip
%...Definitionen der Einrueckungstiefen der einzelnen Sektionen.........
\newcounter{toc@depth}
\newdimen\@toc@total%
\newdimen\@toc@secnum%
\newdimen\@toc@sectotal%
\newdimen\@toc@subsecnum%
\newdimen\@toc@subsectotal%
\newdimen\@toc@subsubsecnum%
\newdimen\@toc@subsubsectotal%
