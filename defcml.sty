\def\every@rahmen{%
   \let\EveryShipout@Hook\@empty%
   \def\Every@Shipout{\g@addto@macro\EveryShipout@Hook}%
   \def\EveryShipout@Shipout{%
      \afterassignment\EveryShipout@Test%
      \global\setbox\@cclv= %
}%
   \def\EveryShipout@Test{%
      \ifvoid\@cclv\relax%
         \aftergroup\EveryShipout@Output%
      \else%
         \EveryShipout@Output%
      \fi%
}%
   \def\EveryShipout@Output{%
      \EveryShipout@Hook%
      \if@portrait@%
         \setbox\@cclv=\vbox{\unvbox\@cclv}%
      \else%
         \setbox\@cclv=\vbox{%
            \vskip-\ltextheight\rot@object{-90}{\box\@cclv}}%
      \fi%
      \EveryShipout@Org@Shipout\box\@cclv%
}%
   \let\EveryShipout@Org@Shipout\@empty%
   \def\EveryShipout@Init{%
      \let\EveryShipout@Org@Shipout\shipout%
      \let\shipout\EveryShipout@Shipout%
}%
   \def\ShipoutPicture{%
      \bgroup%
      \global\setbox\@cclv\vbox{%
         \slide@frame%
         \box\@cclv%
      }%
      \egroup%
}%
   \AtBeginDocument{\EveryShipout@Init}%
   \Every@Shipout{\ShipoutPicture}%
}
