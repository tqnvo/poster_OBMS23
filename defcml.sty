   \refstepcounter{equation}%
   \begingroup%
   \edef\@temp{\def\noexpand\thecurrent@number{\theequation}}%
   \expandafter\endgroup\@temp%
   \setcounter{current@number}{\value{equation}}%
   \setcounter{equation}{0}%
   \def\thesub@num{\subcurrentnumber@{#1}}%
   \def\theequation{#2\thecurrent@number\thesub@num}%
