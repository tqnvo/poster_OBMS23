            \put( 16.00,281.20){\@texta}%
            \put(196.00,281.20){\llap{\@textb}}%
            \put( 16.00, 16.10){\@textc}%
            \put( 16.00, 19.00){\framebox(180.00,260.00){}}%
            \add@to@portrait@frame%
         \end{picture}%
      \else%
         \begin{picture}(0.00,0.00)(25.50,191.30)%
            \put( 16.75,200.70){\@texta}%
            \put(281.75,200.70){\llap{\@textb}}%
            \put( 16.75, 20.60){\@textc}%
            \put( 16.75, 23.50){\framebox(265.00,175.00){}}%
            \add@to@landscape@frame%
         \end{picture}%
      \fi%
   }%
}
