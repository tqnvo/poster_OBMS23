\def\@Thebibliography#1{%
%...Listenumgebung......................................................
   \list{%
%...Fortlaufende Numerierung in eckigen Klammern........................
         \@biblabel{\@arabic\c@enumiv}}{%
%...Mindestabstand Marke - Referenzeintrag..............................
