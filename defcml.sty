\newdimen\@toc@paratotal%
\newdimen\@toc@subparanum
\@toc@total=19.0mm%
\@toc@secnum=16\p@%         Einruecktiefe: section
\@toc@subsecnum=22.5\p@%    Einruecktiefe: subsection
\@toc@subsubsecnum=30\p@%   Einruecktiefe: subsubsection
\@toc@paranum=37\p@%        Einruecktiefe: paragraph
\@toc@subparanum=45\p@%     Einruecktiefe: subparagraph
\def\calc@toc@indent{%
   \@toc@sectotal=\@toc@total%
   \advance\@toc@sectotal by\@toc@secnum%
   \@toc@subsectotal=\@toc@sectotal%
   \advance\@toc@subsectotal by\@toc@subsecnum%
   \@toc@subsubsectotal=\@toc@subsectotal%
   \advance\@toc@subsubsectotal by\@toc@subsubsecnum%
   \@toc@paratotal=\@toc@subsubsectotal%
