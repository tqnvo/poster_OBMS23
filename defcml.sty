%---ERSTELLEN DES INHALTSVERZEICHNISSES---------------------------------
%   Syntax:   \Tableofcontents[#1][#2]  ... fuer Monographien
%             \Tableofcontents*[#1][#2] ... fuer Publikationen
%             #1 ... optionale Vorgabe des Vielfachen  vom normalen Zei-
%                    lenabstand (Stand. 1)
%             #2 ... optionale Vorgabe der max. Sektionstiefe
%                    ( bei Monographien : Stand. 3
%                      bei Publikationen: Stand. 2 )
%-----------------------------------------------------------------------
\newcommand{\Tableofcontents}{\@ifstar{\@tableofcontentsp}{\@tableofcontentsm}}

%---ERSTELLEN DES INHALTSVERZEICHNISSES FUER MONOGRAPHIEN---------------
\def\@tableofcontentsm{\@ifnextchar[\tableofcontents@i{\tableofcontents@ii}}
%...Definitionen der Einrueckungstiefen der einzelnen Sektionen.........
\newdimen\@tocsectotal%
\newdimen\@tocsubsecnum%
\newdimen\@tocsubsectotal%
\newdimen\@tocsubsubsecnum%
\newdimen\@tocsubsubsectotal%
\newdimen\@tocparanum%
\newdimen\@tocparatotal%
\newdimen\@tocsubparanum
\@tocsubsecnum=28\p@%        Einruecktiefe: subsection
\@tocsubsubsecnum=35.5\p@%   Einruecktiefe: subsubsection
\@tocparanum=42.5\p@%        Einruecktiefe: paragraph
\@tocsubparanum=50.5\p@%     Einruecktiefe: subparagraph
\def\calc@tocindent{%
   \@tocsectotal=\z@%        Einruecktiefe: section
   \advance\@tocsectotal by 20\p@%
   \@tocsubsectotal=\@tocsectotal%
   \advance\@tocsubsectotal by\@tocsubsecnum%
   \@tocsubsubsectotal=\@tocsubsectotal%
   \advance\@tocsubsubsectotal by\@tocsubsubsecnum%
   \@tocparatotal=\@tocsubsubsectotal%
   \advance\@tocparatotal by\@tocparanum%
}
%...Makros..............................................................
\def\tableofcontents@i[#1]{%
   \@ifnextchar[{\tableofcontents@ia{#1}}{\tableofcontents@ib{#1}}%
}
\def\tableofcontents@ii{\setcounter{tocdepth}{3}\tableofcontents@{1}}
\def\tableofcontents@ia#1[#2]{%
   \setcounter{tocdepth}{3}%
   \def\@sec@toc@depth{#2}%
   \ifx\@empty\@sec@toc@depth%
   \else%
      \setcounter{tocdepth}{#2}%
   \fi%
   \tableofcontents@{#1}%
}
\def\tableofcontents@ib#1{\setcounter{tocdepth}{3}\tableofcontents@{#1}}
\def\tableofcontents@#1{%
   \begingroup%
%...Einstellung des Zeilenabstandes.....................................
      \baselineskip\ifx #1\@empty\else #1\fi\normalbaselineskip%
%...vertikaler Abstand zum vorangehenden Text...........................
      \par\addvspace{17\p@ plus4\p@ minus4\p@}%
%...Einspaltig Formatierung bei zweiseitiger Seitenformatierung.........
      \@restonecolfalse\if@twocolumn\@restonecoltrue\onecolumn\else\fi%
%...Ueberschrift........................................................
      \centerline{\large\bfseries\@contentsname}%
%...Eintragung der Ueberschrift in ein globales Merkregister............
      \@mkboth{\@contentsname}{\@contentsname}%
%...Tiefe der Sektionsnumerierung.......................................
      \ifnum\value{tocdepth}<1\setcounter{tocdepth}{1}\fi%
      \ifnum\value{tocdepth}>5\setcounter{tocdepth}{5}\fi%
      \calc@tocindent%
%...Abstand der Punkte in der gepunkteten Linie.........................
      \def\@dotsep{3}%
