%...Alter Rahmen........................................................
\newcommand{\rahmen}[4]{%
   \unitlength10mm%
   \if@slide@\vspace*{-8mm}\fi%
   \if@slide@\hskip-10mm\fi%
   \begin{picture}(18,26)%
      \put(18.0,26.2){\llap{#2}}%
      \put( 0.0,26.2){#3}%
      \put( 0.0,-0.3){{\tiny #1}}%
      \put( 0.0, 0.0){%
         \framebox(18,26)[t]{%
            \begin{minipage}[t]{160mm}%
               \vspace*{2cm}#4%
            \end{minipage}%
      }}%
   \end{picture}%
}
%...Alter Querrahmen....................................................
\newcommand{\querrahmen}[4]{%
   \unitlength10mm%
   \if@slide@\hskip-7.5mm\else\hskip3mm\fi%
   \@Rotate{-90}%
   \if@slide@\hskip-15.5mm\fi%
   \begin{picture}(26,0)%
      \put(26.5,17.7){\llap{#2}}%
      \put( 0.0,17.7){#3}%
      \put( 0.0,-0.3){{\tiny #1}}%
      \put( 0.0, 0.0){%
         \framebox(26.5,17.5)[t]{%
            \begin{minipage}[t]{230mm}%
               \vspace*{2cm}#4%
            \end{minipage}%
      }}%
   \end{picture}%
   \@endRotate%
}
