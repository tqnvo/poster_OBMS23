%...Laengenparameter fuer die Sektionsueberschrift......................
\newdimen\headline@width \headline@width\z@
%...Box mit dem Inhaltsverzeichnis als Inhalt...........................
\newbox\tableofcontents@box
%...Makros..............................................................
\def\tableofcontentsp@i[#1]{%
   \@ifnextchar[{\tableofcontentsp@ia{#1}}{\tableofcontentsp@ib{#1}}%
}
\def\tableofcontentsp@ii{\setcounter{toc@depth}{2}\tableofcontentsp@{1}}
\def\tableofcontentsp@ia#1[#2]{%
   \setcounter{toc@depth}{2}%
   \def\@sec@toc@depth{#2}%
   \ifx\@empty\@sec@toc@depth%
   \else%
      \setcounter{toc@depth}{#2}%
   \fi%
   \tableofcontentsp@{#1}%
}
\def\tableofcontentsp@ib#1{\setcounter{toc@depth}{2}\tableofcontentsp@{#1}}
\def\tableofcontentsp@#1{%
   \begingroup%
%...Schriftgroesse......................................................
      \footnotesize%
%...Zeilenabstand.......................................................
      \baselineskip\ifx #1\@empty\else #1\fi\normalbaselineskip%
%...Einspaltige Formatierung bei zweiseitiger Seitenformatierung........
      \@restonecolfalse\if@twocolumn\@restonecoltrue\onecolumn\else\fi%
%...Ueberschrift........................................................
      \centerline{\normalsize\bfseries\@contentsname}%
%...Eintragung der Ueberschrift in ein globales Merkregister............
      \@mkboth{\@contentsname}{\@contentsname}%
%...Abstand Ueberschrift - Inhaltsverzeichnis...........................
      \@tempdima=1em%
      \vskip\ifx #1\@empty\else #1\fi\@tempdima%
%...Tiefe der Sektionsnumerierung.......................................
      \ifnum\value{toc@depth}<1\setcounter{toc@depth}{1}\fi%
      \ifnum\value{toc@depth}>4\setcounter{toc@depth}{4}\fi%
      \calc@toc@indent%
%...Abstand der Punkte in der gepunkteten Linie.........................
      \def\@dotsep{3}%
%...Erstellen der Eintraege in das Inhaltsverzeichnis...................
      \def\l@section{\@toc@line{1}{\@toc@total}{\@toc@secnum}}%
      \def\l@subsection{\@toc@line{2}{\@toc@sectotal}{\@toc@subsecnum}}%
      \def\l@subsubsection{\@toc@line{3}{\@toc@subsectotal}{\@toc@subsubsecnum}}%
      \def\l@paragraph{\@toc@line{4}{\@toc@subsubsectotal}{\@toc@paranum}}%
      \def\l@subparagraph{\@toc@line{5}{\@toc@paratotal}{\@toc@subparanum}}%
%...Verzeichnis (`\@starttoc' bindet die Datei `jobname'.toc ein).......
      \setbox\tableofcontents@box\vbox{\@starttoc{toc}}%
%...horizontale Ausrichtung des Inhaltsverzeichnisses (Zentrieren)......
      \@tempdima\textwidth%
      \advance\@tempdima-2\@toc@total%
      \advance\@tempdima-\headline@width%
      \ifdim\@tempdima>\z@%
         \advance\@tempdima-2\parindent%
         \hskip.5\@tempdima%
      \fi%
      \box\tableofcontents@box%
%...ggf. wird zweispaltige Seitenformatierung wieder hergestellt........
      \if@restonecol\twocolumn\fi%
   \endgroup%
}
%...betrifft Eintraege im Inhaltsverzeichnis............................
\def\@toc@line#1#2#3#4#5{%
%...Sektionstiefe.......................................................
   \ifnum #1>\value{toc@depth}%
   \else%
%...beeinflusst Zeilenumbruch...........................................
      \addpenalty{-\@highpenalty}%
%...Groesse zur Einstellung des Zeilenabstandes.........................
      \@toc@lineskip=0.15mm%
%...Abstand zur vorangehenden Zeile.....................................
      \vskip-1mm\vskip-#1\@toc@lineskip%
      \begingroup%
%...linker Rand......................................................... 
         \leftskip#2\relax%
%...rechter Rand........................................................
         \rightskip\@toc@total%
%...Einrueckung der 1. Zeile............................................
         \parindent#2\relax\@afterindenttrue%
%...horizontaler `skip' am Absatzende...................................
         \parfillskip\z@ plus 1fil%
%...Einrueckung der Sektionsueberschrift................................
         \leavevmode%
         \@tempdima#3\relax\advance\leftskip\@tempdima\hskip-\leftskip%
%...Sektionsueberschrift / Korrektur der Zeilenabstaende................
         \@tempskipa\baselineskip%
         \@tempcnta=100%
         \@tempcntb#1%
         \multiply\@tempcntb by 5%
         \advance\@tempcnta-\@tempcntb%
         \multiply\baselineskip\@tempcnta%
         \divide\baselineskip by 100%
         \ifnum #1=1%
            \setbox8\hbox{\bfseries\boldmath#4.}%
         \else%
            \setbox8\hbox{%
            \ifnum #1=5%
               \ifnum\c@secnumdepth<4%
                  \it%
            \fi\fi#4\/.}%
         \fi%
%...Ermittlung der maximalen Breite des Inhaltsverzeichnisses...........
         \@tempdima\wd8%
         \unhbox8\hfill%
         \advance\@tempdima by #2%
         \advance\@tempdima-\@toc@total%
         \ifdim\headline@width<\@tempdima%
            \global\headline@width\@tempdima%
         \fi%
%...neuer Absatz........................................................
         \nobreak\par%
%...Initialisierung der Zeilenabstaende.................................
         \baselineskip\@tempskipa%
      \endgroup%
%...beeinflusst Zeilenumbruch...........................................
      \penalty\@highpenalty%
   \fi%
}

%---KOPF DER TITELSEITE-------------------------------------------------
%   Syntax:   \Maketitle
%   Es muessen a priori folgende Parameter im Text definiert werden:
%   \title{#1}         ... Titel #1 der Publikation
%   \author{#1}        ... Autoren #1
%   \address{#1}       ... Anschriften #1 der Autoren  (kursiv hervorge-
%                          hoben)
%   \abstract[#1]{#2}  ... Zusammenfassung #3,  die optional um #1 beid-
%                          seitig eingerueckt wird
%   \thanks{#1}        ... Fussnotentext #1
%-----------------------------------------------------------------------
\newcommand{\Maketitle}{%
   \par%
   \begingroup%
      \renewcommand\thefootnote{\fnsymbol{footnote}}%
      \def\@makefnmark{\hbox to\z@{$\m@th^{\@thefnmark}$\hss}}%
      \long\def\@makefntext##1{\parindent1em\noindent%
                       \hbox to1.8em{\hss$\m@th^{\@thefnmark}$}##1}%
      \if@twocolumn%
         \ifnum\col@number=\@ne%
            \@Maketitle%
         \else%
            \twocolumn[\@Maketitle]%
         \fi%
      \else%
         \newpage%
         \global\@topnum\z@%
         \@Maketitle%
      \fi%
      \thispagestyle{empty}\@thanks%
   \endgroup%
   \setcounter{footnote}{0}%
   \let\Maketitle\relax%
   \let\@Maketitle\relax%
   \let\thanks\relax%
   \gdef\@title{}%
   \gdef\@author{}%
   \gdef\@address{}%
   \gdef\@abstract{}%
   \gdef\@thanks{}%
}
\def\@Maketitle{%
   \newpage%
   \if@twocolumn%
      \vskip-1em%
   \fi%
  {\centering%
      \ifx\@title\@empty%
      \else%
         \vskip1.5em%
        {\large\bfseries\boldmath\@title\par}%
      \fi%
      \ifx\@author\@empty%
      \else%
         \vskip1.5em%
        {\lineskip.5em%
           {\setlength{\tabcolsep}{0in}%
            \begin{tabular}[t]{c}%
               \@author%
            \end{tabular}\par}%
        }%
      \fi%
      \ifx\@address\@empty%
      \else%
         \vskip1.5em%
        {\lineskip.5em%
           {\setlength{\tabcolsep}{0in}%
            \itshape%
            \begin{tabular}[t]{c}%
               \@address%
            \end{tabular}\par}%
        }%
      \fi%
   }%
   \@abstract%
}
\def\abstract{\@ifnextchar[\abstract@{\abstract@[.75cm]}}
\long\def\abstract@[#1]#2{%
   \def\contents@abstract{#2}%
   \gdef\@abstract{\@restonecolfalse%
                   \if@twocolumn\@restonecoltrue\@twocolumnfalse\fi%
                   \ifx\contents@abstract\@empty%
                      \if@restonecol\vspace{1.5em}\else\vspace{0.5em}\fi%
                   \else%
                      \begin{Abstract}[#1]#2\end{Abstract}%
                      \if@restonecol\twocolumn\fi%
                   \fi}%
}
