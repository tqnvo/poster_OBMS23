%---`OVER'-KOMMANDO UND BRUECHE IM `DISPLAYSTYLE' UND `TEXTSTYLE'-------
%    Mit dem `over'-Kommando kann  z.B. ein binomischer Ausdruck erzeugt
%    werden;
%    Syntax:    \dispover{#1}{#2}{#3} ... im `displaystyle'
%               \textover{#1}{#2}{#3} ... im `textstyle'
%               #1 ... Klammersymbole: #1=1 ->       ( ## )
%                                      #1=2 ->       [ ## ]
%                                      #1=3 ->       { ## }
%                                      #1=4 -> \lgroup ## \rgroup
%                                      #1=5 ->         ##
%               #2 ... Platzhalter, welcher oben     platziert wird
%               #3 ... Platzhalter, welcher darunter platziert wird
%
%               \dispfrac{#1}{#2}     ... im `displaystyle'
%               \textfrac{#1}{#2}     ... im `textstyle'
%               #1 ... Zaehler
%               #2 ... Nenner
%-----------------------------------------------------------------------
\newcommand{\dispover}[3]{%
   \over@delim{#1}%
   \mkern.5mu%
   \left@delim\matrix{#2}\\{#3}\endmatrix\right@delim%
   \mkern.5mu%
}
\newcommand{\textover}[3]{%
   \over@delim{#1}%
   \mkern.5mu%
   \expandafter\left@delim\csname submatrix*\endcsname{#2}\\{#3}%
   \expandafter\relax\csname endsubmatrix*\endcsname\right@delim%
   \mkern.5mu%
}
\newcommand{\dispfrac}[2]{\mkern.5mu{\displaystyle\frac{#1}{#2}}\mkern.5mu}
\newcommand{\textfrac}[2]{\mkern.5mu{\textstyle\frac{#1}{#2}}\mkern.5mu}
\def\over@delim#1{%
   \ifnum #1=1%
      \def\left@delim{\left(}%
      \def\right@delim{\right)}%
   \else%
      \ifnum #1=2%
         \def\left@delim{\left[}%
         \def\right@delim{\right]}%
      \else%
         \ifnum #1=3%
            \def\left@delim{\left\{}%
            \def\right@delim{\right\}}%
         \else%
            \ifnum #1=4%
               \def\left@delim{\left\lgroup}%
               \def\right@delim{\right\rgroup}%
            \else%
               \ifnum #1=5%
                  \def\left@delim{}%
                  \def\right@delim{}%
               \else%
                  \err@delim{#1}%
   \fi\fi\fi\fi\fi%
}
\def\err@delim#1{%
   \error@package{^^J%
      Delimiters with `#1' not definable}%
}

