\def\Rahmen@[#1]#2#3#4{%
   \newpage%
   \global\@portrait@true%
   \portrait@%
   \frame@line{#1}{#2}{#3}{#4}%
   \vbox to\z@\bgroup%
      \vbox\bgroup%
         \text@layout%
         \vbox to\z@{\rlap{\slide@frame}}%
         \nointerlineskip\kern0.5mm%
         \leftskip+0.5mm%
         \rightskip-\leftskip%
}
\def\endRahmen@{%
      \egroup\vss%
   \egroup%
}
\def\Rahmen@every[#1]#2#3#4{%
   \newpage%
   \global\@portrait@true%
   \portrait@%
   \frame@line{#1}{#2}{#3}{#4}%
   \text@layout%
}
\def\Querrahmen@[#1]#2#3#4{%
   \newpage%
   \global\@portrait@false%
   \frame@line{#1}{#2}{#3}{#4}%
   \landscape@%
   \setbox\Querrahmen@box\vbox to\z@\bgroup%
      \vbox\bgroup%
         \text@layout%
         \vskip-\ltextheight\vskip2mm%
         \vbox to\z@{\hskip-1.75mm\rlap{\slide@frame}}%
         \nointerlineskip\kern-1mm%
         \leftskip-0.5mm%
         \rightskip-\leftskip%
}
\def\endQuerrahmen@{%
      \egroup\vss%
   \egroup%
   \rot@object{-90}{\box\Querrahmen@box}%
}
\def\Querrahmen@every[#1]#2#3#4{%
   \newpage%
   \global\@portrait@false%
   \landscape@%
   \frame@line{#1}{#2}{#3}{#4}%
   \text@layout%
}
\def\frame@line#1#2#3#4{%
   \linethickness{#1}%
   \ifdim #1=\z@%
      \slide@head@foot{}{}{}%
   \else%
      \slide@head@foot{#2}{#3}{#4}%
   \fi%
}
\def\slide@head@foot#1#2#3{%
   \toks@{\sffamily\large#1}%
   \xdef\@texta{\the\toks@}%
   \toks@{\sffamily\large#2}%
   \xdef\@textb{\the\toks@}%
   \toks@{\sffamily\scriptsize#3}%
   \xdef\@textc{\the\toks@}%
}
\def\add@to@slide@frame@(#1,#2){%
   \@ifnextchar[{\add@to@slide@frame@@{#1}{#2}}{%
   \add@to@slide@frame@@{#1}{#2}[]}}
\long\def\add@to@slide@frame@@#1#2[#3]#4{%
   \@tempdima #1\p@%
   \@tempdimb #2\p@%
   \if@portrait@%
      \advance\@tempdima by 16.00\p@%
      \advance\@tempdimb by 19.00\p@%
      \edef\@tempa{\remove@pt\@tempdima}%
      \edef\@tempb{\remove@pt\@tempdimb}%
      \xdef\add@to@portrait@frame{%
         \add@to@portrait@frame%
         \put(\@tempa,\@tempb){\text@pos{#3}{#4}}}%
   \else%
      \advance\@tempdima by 16.75\p@%
      \advance\@tempdimb by 23.50\p@%
      \edef\@tempa{\remove@pt\@tempdima}%
      \edef\@tempb{\remove@pt\@tempdimb}%
      \xdef\add@to@landscape@frame{%
         \add@to@landscape@frame%
         \put(\@tempa,\@tempb){\text@pos{#3}{#4}}}%
   \fi%
}
\def\text@pos#1#2{%
   \ifx r#1\hb@xt@\z@{\hss#2}\else%
   \ifx c#1\hb@xt@\z@{\hss#2\hss}\else%
           \hb@xt@\z@{#2\hss}%
   \fi\fi%
}
\def\slide@frame{%
   \vbox{%
